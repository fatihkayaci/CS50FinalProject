CREATE TABLE IF NOT EXISTS 'text_tbl' (
        id INTEGER NOT NULL,
        group_name VARCHAR(50) NOT NULL,
        id_name VARCHAR(50) NOT NULL,
        text VARCHAR(5000), label VARCHAR(100), page_name VARCHAR(50),
        PRIMARY KEY (id)
    );