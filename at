CREATE TABLE tblmedia (
        id INTEGER NOT NULL,
        image_path VARCHAR(50),
        page_name VARCHAR(50),
        title VARCHAR(100),
        PRIMARY KEY (id)
);
CREATE TABLE tbltext (
        id INTEGER NOT NULL,
        text_message VARCHAR(5000),
        page_name VARCHAR(50),
        title VARCHAR(100),
        PRIMARY KEY (id)
);
